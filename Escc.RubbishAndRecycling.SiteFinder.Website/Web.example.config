<configuration>
  <configSections>
    <section name="exceptionless" type="Exceptionless.Configuration.ExceptionlessSection, Exceptionless" />
    <sectionGroup name="EsccWebTeam.Egms">
      <section name="CssFiles" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="EgmsWebMetadata" type="EsccWebTeam.Egms.EgmsWebMetadataSectionHandler,EsccWebTeam.Egms, Version=1.0.0.0, Culture=neutral, PublicKeyToken=06fad7304560ae6f" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="EsccWebTeam.EastSussexGovUK">
      <section name="GeneralSettings" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="BreadcrumbTrail" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="DesktopMasterPages" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="MobileMasterPages" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="RemoteMasterPage" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
    </sectionGroup>
    <sectionGroup name="EsccWebTeam.Data.Xml">
      <section name="Proxy" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
    </sectionGroup>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Escc.FormControls.WebForms.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <applicationSettings>
    <Escc.FormControls.WebForms.Properties.Settings>
      <setting name="Escc_FormControls_WebForms_AddressFinder_AddressFinder"
     serializeAs="String">
        <value>URL of Address Finder web service</value>
      </setting>
    </Escc.FormControls.WebForms.Properties.Settings>
  </applicationSettings>

  <appSettings>
    <add key="RecyclingSiteFinderBaseUrl" value="https://hostname/WasteSearch.aspx"/>
    <add key="RecyclingSiteDataUrl" value="https://hostname/umbraco/api/location/list?type=RecyclingSite"/>
    <add key="WasteTypesDataUrl" value="https://hostname/umbraco/api/wastetypes/list"/>
    <!--<add key="ConnectToCouncilWebServicesAccount" value="domain\account"/>
    <add key="ConnectToCouncilWebServicesPassword" value="password"/>-->
  </appSettings>
  
  <exceptionless apiKey="API_KEY_HERE" />
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <pages masterPageFile="~/escc.eastsussexgovuk/masterpages/desktop.master">
      <controls>
        <add tagPrefix="EastSussexGovUK" namespace="EsccWebTeam.EastSussexGovUK" assembly="EsccWebTeam.EastSussexGovUK" />
        <add tagPrefix="EastSussexGovUK" tagName="Related" src="~/escc.eastsussexgovuk/masterpages/controls/related.ascx" />
        <add tagPrefix="EastSussexGovUK" tagName="Share" src="~/escc.eastsussexgovuk/masterpages/controls/share.ascx" />
        <add tagPrefix="Egms" namespace="EsccWebTeam.Egms" assembly="EsccWebTeam.Egms" />
        <add tagPrefix="NavigationControls" namespace="EsccWebTeam.NavigationControls" assembly="EsccWebTeam.NavigationControls" />
      </controls>
      <tagMapping>
        <add tagType="System.Web.UI.WebControls.Button" mappedTagType="Escc.FormControls.WebForms.EsccButton, Escc.FormControls.WebForms" />
      </tagMapping>
    </pages>
  </system.web>
  <system.webServer>
    <modules>
      <add name="MasterPageModule" type="EsccWebTeam.EastSussexGovUK.MasterPages.MasterPageModule, EsccWebTeam.EastSussexGovUK" />
      <add name="ExceptionlessModule" type="Exceptionless.Web.ExceptionlessModule, Exceptionless.Web" />
    </modules>
  </system.webServer>

 <!-- <EsccWebTeam.Data.Xml>
    <Proxy>
      <add key="Server" value="ip address" />
      <add key="User" value="user" />
      <add key="Domain" value="domain" />
      <add key="Password" value="password" />
    </Proxy>
  </EsccWebTeam.Data.Xml>-->

  <EsccWebTeam.EastSussexGovUK>
    <GeneralSettings>
      <add key="PlainMasterPage" value="~/escc.eastsussexgovuk/masterpages/plain.master" />
      <add key="MasterPageParameterName" value="template" />
      <add key="ErrorControlUrl" value="~/escc.eastsussexgovuk/masterpages/controls/error{0}.ascx" />
    </GeneralSettings>
    <BreadcrumbTrail>
      <add key="Home" value="/" />
      <add key="Environment &amp; planning" value="/environment/" />
      <add key="Rubbish and recycling" value="/environment/rubbishandrecycling/" />
      <add key="Recycling sites" value="/environment/rubbishandrecycling/recyclingsites/" />
    </BreadcrumbTrail>
    <DesktopMasterPages>
      <add key="/" value="~/escc.eastsussexgovuk/masterpages/desktop.master" />
    </DesktopMasterPages>
    <MobileMasterPages>
      <add key="/" value="~/escc.eastsussexgovuk/masterpages/mobile.master" />
    </MobileMasterPages>
      <RemoteMasterPage>
      <add key="CacheMinutes" value="60" />
			<add key="MasterPageControlUrl" value="https://www.eastsussex.gov.uk/masterpages/remote/control.aspx?control={0}" />
    </RemoteMasterPage>
  </EsccWebTeam.EastSussexGovUK>


  <EsccWebTeam.Egms>
    <EgmsWebMetadata creator="Web Team" errorMode="On" titlePattern="{0} &amp;#8211; East Sussex County Council" />
    <CssFiles>
      <add key="Medium" value="only screen and (min-width: 474px)" />
      <add key="Large" value="only screen and (min-width: 802px)" />

      <add key="FormsSmall" value="/escc.eastsussexgovuk/css/forms-small.css" />
      <add key="FormsMedium" value="/escc.eastsussexgovuk/css/forms-medium.css" />
      <add key="FormsLarge" value="/escc.eastsussexgovuk/css/forms-large.css" />
    </CssFiles>
  </EsccWebTeam.Egms>
</configuration>